<p id="notice"><%= notice %></p>

<p>
  <strong>Number:</strong>
  <%= @dover.number %>
</p>

<p>
  <strong>Startdate:</strong>
  <%= @dover.startdate %>
</p>

<p>
  <strong>Enddate:</strong>
  <%= @dover.enddate %>
</p>
<p>
  <strong>Subj:</strong>
  <%= @dover.subj %>
</p>

<p>
  <strong>Company:</strong>
  <%= @dover.company.title %>
</p>

<p>
  <strong>User:</strong>
  <%= @dover.user.first_name %>
</p>

<div class="col-md-12">
<%= link_to t('edit'), edit_dover_path(@dover), class: 'btn btn-default btn-sm' %> 
<%= link_to t('back'), dovers_path, class: 'btn btn-default btn-sm' %>
<%= link_to '<i class="glyphicon glyphicon-print"></i>'.html_safe, print_dover_path(@dover), target: :_blank, class: 'btn btn-success' %>
