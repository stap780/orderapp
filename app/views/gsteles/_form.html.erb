<%= simple_form_for(@gstele) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :sku %>
    <%= f.input :title %>
    <%= f.input :quantity %>
    <%= f.input :valute %>
    <%= f.input :cost_price %>
    <%= f.input :price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
