<p id="notice"><%= notice %></p>

<h1>Listing Cars</h1>
<div class="col-md-4">
		<p>Import product</p>
		<p><%= form_tag import_cars_path, multipart: true do %></p>  <p><%= file_field_tag :file %></p>  <p><%= submit_tag "Import" %>
		<% end %>
		</p>
</div>
<div class="col-md-3">
			| <%= link_to 'Обновить id', webidupdate_cars_path%> | 
</div>
<div class="col-md-3">
			| <%= link_to 'Обновить url', uploadurl_cars_path%> | 
</div>
<div class="col-md-12">

<table>
  <thead>
    <tr>
      <th>Webid</th>
      <th>Title</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.webid %></td>
        <td><%= car.title %></td>
        <td><%= car.url %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
</div>